    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "bulma": "^0.8.2",
    "classnames": "^2.2.6",
    "fuzzysort": "^1.1.4",
    "graphql": "14.5.8",
    "graphql-tag": "^2.10.3",
    "immer": "^6.0.3",
    "lodash": "^4.17.15",
    "phoenix": "^1.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2",
    "zen-observable-ts": "^0.8.21"
  },
  "devDependencies": {
    "@testing-library/react": "^9.3.2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/phoenix": "^1.4.4",
